{
  "name": "eazytable-workspace",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "pnpm -r --filter './packages/*' build",
    "dev": "pnpm -r --filter './packages/*' dev",
    "typecheck": "pnpm -r --filter './packages/*' typecheck",
    "lint": "pnpm -r --filter './packages/*' lint",
    "clean": "pnpm -r --filter './packages/*' clean"
  },
  "devDependencies": {
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^19.0.0",
      "@types/react-dom": "^19.0.0"
    }
  }
}
